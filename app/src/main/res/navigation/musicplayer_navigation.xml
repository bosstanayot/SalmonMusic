<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/musicplayer_navigation"
    app:startDestination="@id/playerFragment">
    <fragment
        android:id="@+id/playerFragment"
        android:name="tanayot.barjord.musicplayerapplication.ui.PlayerFragment"
        tools:layout="@layout/player_fragment"
        android:label="PlayerFragment" >

        <action
            android:id="@+id/action_playerFragment_to_playerFragment2"
            app:destination="@id/playerFragment2" />
    </fragment>

    <fragment
        android:id="@+id/playerFragment2"
        android:name="tanayot.barjord.musicplayerapplication.ui.PlayerFragment"
        tools:layout="@layout/player_fragment"
        android:label="PlayerFragment" >
        <argument android:name="songData" app:argType="tanayot.barjord.musicplayerapplication.model.Song"/>
    </fragment>
</navigation>